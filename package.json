{
  "name": "x-value",
  "version": "0.0.0",
  "repository": "https://github.com/vilic/x-value.git",
  "license": "MIT",
  "author": "vilicvane",
  "type": "module",
  "exports": {
    ".": {
      "types": "./bld/library/index.d.ts",
      "default": "./bld/library/index.js"
    },
    "./node": {
      "types": "./bld/node/index.d.ts",
      "default": "./bld/node/index.js"
    }
  },
  "scripts": {
    "3": "yarn && yarn-deduplicate && yarn",
    "build": "rimraf ./bld && tsc --build",
    "lint": "eslint --no-error-on-unmatched-pattern . && run-in-every eslint-project --parallel --echo -- eslint --no-error-on-unmatched-pattern .",
    "lint-prettier": "prettier --check .",
    "test": "yarn lint-prettier && yarn build && yarn lint"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@mufan/eslint-plugin": "^0.2.0",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "run-in-every": "^0.2.0",
    "typescript": "^5.2.2",
    "yarn-deduplicate": "^6.0.2"
  }
}
